{
  "name": "postcode-nl/api-client",
  "description": "A client to the Postcode.nl Address Api for international address validation. You will need to create an account at https://api.postcode.nl.",
  "keywords": [
    "postcode",
    "netherlands",
    "germany",
    "belgium",
    "api",
    "rest",
    "client",
    "address",
    "autocomplete"
  ],
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Postcode.nl B.V.",
      "role": "Developer"
    }
  ],
  "homepage": "https://api.postcode.nl",
  "support": {
    "email": "tech@postcode.nl",
    "issues": "https://github.com/postcode-nl/PostcodeNl_Api_Client/issues",
    "support": "https://github.com/postcode-nl/PostcodeNl_Api_Client"
  },
  "autoload": {
    "psr-4": {
      "PostcodeNl\\Api\\": "src"
    }
  },
  "replace": {
    "postcode-nl/api-restclient": "*"
  },
  "require": {
    "php": "^7.1",
    "ext-curl": "*",
    "ext-json": "*"
  }
}
